{
  "name": "Theme IUH",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://sv.iuh.edu.vn/*"],
      "js": ["scripts.js"],
      "css": ["style/css/theme.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["style/css/theme.css"],
      "matches": ["*://sv.iuh.edu.vn/*"],
      "use_dynamic_url": true
    }
  ],
  "action": {}
}
